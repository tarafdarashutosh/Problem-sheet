1) problem link -  https://bit.ly/3QfK2k3

#include<bits/stdc++.h>
using namespace std;
void printpattern(int n){
for(int i = 1; i<=n; i++){
for(int j = 1; j<=n; j++){
cout<<"*";
}
cout<<endl;
}
}
int main(){
int n;
cin>>n;
printpattern(n);
return 0;
}

problem 2: https://bit.ly/3VADLAt

#include<bits/stdc++.h>
using namespace std;
void printpattern(int n){
for(int i = 1; i<=n;i++){
for(int j = 1 ;j<=i;j++){
cout<<"* ";
}
cout<<endl;
}
}
int main(){
int n;
cin>>n;
printpattern(n);
return 0;
}

Pattern3: https://bit.ly/3CiWV74

#include<bits/stdc++.h>
using namespace std;
void printpattern(int n){
for(int i = 1; i<=n;i++){
for(int j = 1 ;j<=i;j++){
cout<<j<<" ";
}
cout<<endl;
}
}
int main(){
int n;
cin>>n;
printpattern(n);
return 0;
}

Pattern4: https://bit.ly/3Gzv70S

#include<bits/stdc++.h>
using namespace std;
void printpattern(int n){
for(int i = 1; i<=n;i++){
for(int j = 1 ;j<=i;j++){
cout<<i<<" ";
}
cout<<endl;
}
}
int main(){
int n;
cin>>n;
printpattern(n);
return 0;
}

Pattern5: https://bit.ly/3WXGSDD

#include<bits/stdc++.h>
using namespace std;
void printpattern(int n){
for(int i = 1; i<=n;i++){
for(int j = 1 ;j<=n+1-i;j++){
cout<<"* ";
}
cout<<endl;
}
}
int main(){
int n;
cin>>n;
printpattern(n);
return 0;
}
